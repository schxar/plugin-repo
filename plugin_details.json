      "repository_url": "https://github.com/MaiCore-with-u/maibot",
      "keywords": [
        "frequency",
        "control",
        "focus_value",
        "talk_frequency",
        "plugin",
        "shortcut"
      ],
      "categories": [
        "Chat",
        "Frequency",
        "Control"
      ],
      "default_locale": "zh-CN",
      "locales_path": "_locales",
      "plugin_info": {
        "is_built_in": false,
        "plugin_type": "frequency",
        "components": [
          {
            "type": "command",
            "name": "set_focus_value",
            "description": "设置当前聊天的focus_value调整值",
            "pattern": "/chat focus_value <数字> 或 /chat f <数字>"
          },
          {
            "type": "command",
            "name": "set_talk_frequency",
            "description": "设置当前聊天的talk_frequency调整值",
            "pattern": "/chat talk_frequency <数字> 或 /chat t <数字>"
          },
          {
            "type": "command",
            "name": "show_frequency",
            "description": "显示当前聊天的频率控制状态",
            "pattern": "/chat show 或 /chat s"
          }
        ],
        "features": [
          "设置focus_value调整值",
          "设置talk_frequency调整值",
          "显示当前频率控制状态",
          "实时频率控制调整",
          "命令执行反馈（不保存消息）",
          "支持完整命令和简化命令",
          "快速操作支持"
        ]
      }
    }
